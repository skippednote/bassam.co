---
renderData:
  title: '{{ tag }}'
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - posts
    - features
    - tagList
    - links
permalink: /tags/{{ tag | slug }}/
layout: layouts/base.njk
---
<h1>{{ tag }}</h1>

{% set posts = collections[ tag ] | filterBlogPosts | reverse %}
{% include "posts.njk" %}
